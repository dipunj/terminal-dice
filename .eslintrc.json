{
	"root": true,
	"env": {
		"browser": false,
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"airbnb-base",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [ "prettier" ],
	"rules": {
		"prettier/prettier": [
			"error"
		]
	}
}